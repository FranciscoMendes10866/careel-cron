{
  "name": "careel-cron",
  "version": "1.0.0",
  "main": "server.js",
  "license": "CC0-1.0",
  "scripts": {
    "update": "yarn upgrade --latest",
    "dev": "clear && ts-node-dev --cls -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify --files ./src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored",
    "serve": "node ./build/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix src --ext .ts",
    "prisma:studio": "prisma studio",
    "prisma:intro": "prisma introspect",
    "prisma:save": "prisma migrate save --experimental --name",
    "prisma:migrate": "prisma migrate up --experimental"
  },
  "dependencies": {
    "@prisma/cli": "^2.7.1",
    "@prisma/client": "^2.7.1",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "koa": "^2.13.0",
    "moment": "^2.28.0",
    "pg": "^8.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cron": "^1.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/koa": "^2.11.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
